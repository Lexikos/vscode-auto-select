{
  "name": "autohotkey-auto-select",
  "displayName": "AutoHotkey Auto-Select",
  "description": "Automatically switches between the \"ahk\" and \"ahk2\" languages, when detected.",
  "version": "0.0.1",
  "publisher": "lexikos",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
		"Programming Languages"
	],
	"keywords": [
    "ahk",
		"ahk2",
    "autohotkey",
    "AutoHotkey",
		"autohotkey2",
		"AutoHotkey2"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Lexikos/vscode-auto-select"
	},
  "activationEvents": [
    "onLanguage:ahk2",
    "onLanguage:ahk"
  ],
  "main": "./out/extension.js",
  "contributes": {
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  }
}
